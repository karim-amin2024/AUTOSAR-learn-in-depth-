<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00046.xsd ">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>RunnableSum_Lab2</SHORT-NAME>

			<ELEMENTS>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Root_Composition</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Composition_PPort</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/RunnableSum_Lab2/SPI_output</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Composition_RPort</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/RunnableSum_Lab2/SRI_inputs</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>SUM_SWC</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/RunnableSum_Lab2/SUM_SWC_type</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>

					<CONNECTORS>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>input_port_connector</SHORT-NAME>
							<INNER-PORT-IREF>
								<R-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF
										DEST="SW-COMPONENT-PROTOTYPE">/RunnableSum_Lab2/Root_Composition/SUM_SWC</CONTEXT-COMPONENT-REF>
									<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/RunnableSum_Lab2/SUM_SWC_type/RP_SUM</TARGET-R-PORT-REF>
								</R-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="R-PORT-PROTOTYPE">/RunnableSum_Lab2/Root_Composition/Composition_RPort</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>port_output_connector</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF
										DEST="SW-COMPONENT-PROTOTYPE">/RunnableSum_Lab2/Root_Composition/SUM_SWC</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/RunnableSum_Lab2/SUM_SWC_type/PP_SUM</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/RunnableSum_Lab2/Root_Composition/Composition_PPort</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>

				</COMPOSITION-SW-COMPONENT-TYPE>
				<!-- Interfaces -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_inputs</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>input_param1</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>input_param2</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SPI_output</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>SumResult</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SWCs -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SUM_SWC_type</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SUM</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/RunnableSum_Lab2/SPI_output</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_SUM</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/RunnableSum_Lab2/SRI_inputs</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>Sum_Behavior</SHORT-NAME>
							<EVENTS>
								<TIMING-EVENT>
									<SHORT-NAME>SUM_Event_1ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY"></START-ON-EVENT-REF>
								</TIMING-EVENT>
							</EVENTS>
							<RUNNABLES>
								<RUNNABLE-ENTITY>
									<SHORT-NAME>SUM_Runnable_1ms</SHORT-NAME>
									<DATA-READ-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Read_input2_access</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/RunnableSum_Lab2/SUM_SWC_type/RP_SUM</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/RunnableSum_Lab2/SRI_inputs/input_param2</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>Read_input1_access</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="R-PORT-PROTOTYPE">/RunnableSum_Lab2/SUM_SWC_type/RP_SUM</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/RunnableSum_Lab2/SRI_inputs/input_param1</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-READ-ACCESSS>
									<DATA-WRITE-ACCESSS>
										<VARIABLE-ACCESS>
											<SHORT-NAME>write_access_output</SHORT-NAME>
											<ACCESSED-VARIABLE>
												<AUTOSAR-VARIABLE-IREF>
													<PORT-PROTOTYPE-REF
														DEST="P-PORT-PROTOTYPE">/RunnableSum_Lab2/SUM_SWC_type/PP_SUM</PORT-PROTOTYPE-REF>
													<TARGET-DATA-PROTOTYPE-REF
														DEST="VARIABLE-DATA-PROTOTYPE">/RunnableSum_Lab2/SPI_output/SumResult</TARGET-DATA-PROTOTYPE-REF>
												</AUTOSAR-VARIABLE-IREF>
											</ACCESSED-VARIABLE>
										</VARIABLE-ACCESS>
									</DATA-WRITE-ACCESSS>
									<SYMBOL>Sum_1ms</SYMBOL>
								</RUNNABLE-ENTITY>
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
					</INTERNAL-BEHAVIORS>
				</APPLICATION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>